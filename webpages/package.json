{
  "name": "notjustidea-webpage",
  "version": "0.1.0",
  "scripts": {
    "bundle:dev": "webpack --config webpack.dev.config.js",
    "bundle:dist": "webpack --config webpack.dist.config.js",
    "build:dev": "node build/build.js --dev -p dist/*.js -i src/layout/main.tpl -o pages",
    "build:dist": "node build/build.js -p dist/*.js -i src/layout/main.tpl -o pages",
    "watch": "webpack --watch --config webpack.dev.config.js & gazeer -p dist/*.js -c 'node build/build.js --dev -p \"$filepath\" -i src/layout/main.tpl -o pages'"
  },
  "author": "Terry King <terrykingcha@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "art-template": "^3.0.3",
    "autoprefixer": "^6.0.3",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-runtime": "^5.8.38",
    "bluebird": "^3.4.1",
    "consolidate": "^0.14.0",
    "css-loader": "^0.23.1",
    "de-indent": "^1.0.2",
    "eslint": "^2.13.0",
    "fs-extra": "^0.30.0",
    "fs-extra-promise": "^0.4.0",
    "gazeer": "^0.1.1",
    "globule": "^1.0.0",
    "hash-sum": "^1.0.2",
    "loader-utils": "^0.2.10",
    "lru-cache": "^2.7.0",
    "npmlog": "^3.1.2",
    "object-assign": "^4.0.0",
    "parse5": "^2.1.5",
    "postcss": "^5.0.10",
    "postcss-cssnext": "^2.7.0",
    "postcss-selector-parser": "^1.1.2",
    "rollup": "^0.31.2",
    "rollup-plugin-babel": "^2.5.1",
    "rollup-plugin-commonjs": "^3.0.0",
    "rollup-plugin-node-resolve": "^1.7.0",
    "rollup-watch": "^2.4.0",
    "source-map": "^0.5.3",
    "style-loader": "^0.13.1",
    "vue-hot-reload-api": "^2.0.5",
    "vue-html-loader": "^1.2.3",
    "vue-style-loader": "^1.0.0",
    "vue-template-validator": "^1.0.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "yargs": "^4.7.1"
  },
  "dependencies": {
    "es6-promise": "^3.2.1",
    "lodash": "^4.13.1"
  }
}
